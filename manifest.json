{

  "manifest_version": 2,
  "name": "DM Drawer Be Gone",
  "version": "1.0",

  "description": "Removes the ugly little dm drawer on twitter",

  "icons": {
    "48": "icons/box-48.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.twitter.com/*"],
      "js": ["boxRemover.js"]
    }
  ]

}